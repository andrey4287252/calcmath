    "import numpy as np\n",
    "\n",
    "def scal(x,y):\n",
    "    m = x@(y.transpose())\n",
    "    return m\n",
    "\n",
    "F = np.array([[np.cos(i*3.14159*(1 + 2*j)/8) for j in range(4)] for i in range(4)])\n",
    "y = np.array([2, 1, 3, 0])\n",
    "for i in range(4):\n",
    "    print(\"i: \", i, \"answ: \", (1/2)*scal(F[i], y))"
   ]
